{
  "name": "openvpn-riseup",
  "description": "measure vpn connection to random riseup gateways",
  "author": "Ain Ghazal <ain@openobservatory.org>",
  "nettests": [
    {
      "test_name": "openvpn",
      "inputs": [
        "vpn://openvpn.riseup/?addr=199.254.238.55:80&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=204.13.164.252:1194&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=185.220.103.11:1194&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=185.220.103.11:1194&transport=udp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.250:1194&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.250:1194&transport=udp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.248:1194&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.248:1194&transport=udp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.248:80&transport=tcp&obfs=none",
        "vpn://openvpn.riseup/?addr=37.218.244.248:80&transport=udp&obfs=none"
      ],
      "options": {
        "Cipher": "AES-256-GCM",
        "Auth": "SHA512",
        "Compress": "",
        "Obfuscation": "none",
        "SafeProxyURI": "",
        "SafeCa": "base64:",
        "SafeCert": "base64:",
        "SafeKey": "base64:",
        "SafeLocalCreds": false
      }
    }
  ]
}
