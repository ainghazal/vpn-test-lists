{
  "name": "openvpn-riseup",
  "description": "measure vpn connection to random riseup gateways",
  "author": "Ain Ghazal <ain@openobservatory.org>",
  "nettests": [
    {
      "test_name": "openvpn",
      "inputs": [
        "vpn://openvpn.riseup/?addr=80.78.22.247:443&transport=tcp&obfs=obfs4"
      ],
      "options": {
        "PingCount": "10",
        "WithSpeedTest": "10mb",
        "Cipher": "AES-256-GCM",
        "Auth": "SHA512",
        "Compress": "",
        "Obfuscation": "obfs4",
        "SafeProxyURI": "obfs4://80.78.22.247:443/?cert=8nuAbPJwFrKc%2F29KcCfL5LBuEWxQrjBASYXdUbwcm9d9pKseGK4r2Tg47e23%2Bt6WghxGGw&iat-mode=0",
        "SafeCa": "base64:",
        "SafeCert": "base64:",
        "SafeKey": "base64:",
        "SafeLocalCreds": false
      }
    },
    {
      "test_name": "openvpn",
      "inputs": [
        "vpn://openvpn.riseup/?addr=80.78.22.247:995&transport=tcp&obfs=none"
      ],
      "options": {
        "PingCount": "10",
        "WithSpeedTest": "10mb",
        "Cipher": "AES-256-GCM",
        "Auth": "SHA512",
        "Compress": "",
        "Obfuscation": "none",
        "SafeCa": "base64:",
        "SafeCert": "base64:",
        "SafeKey": "base64:",
        "SafeLocalCreds": false
      }
    }
  ]
}
